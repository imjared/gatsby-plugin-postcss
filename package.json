{
  "name": "gatsby-plugin-postcss",
  "description": "Gatsby plugin to handle Postcss syntax",
  "version": "1.0.12",
  "author": "Jared Laser <laserjn@gmail.com>",
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "gatsby-1-config-css-modules": "^1.0.5"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
